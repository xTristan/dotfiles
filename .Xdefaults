URxvt.termName:		    rxvt-256color

Xft.dpi:		    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

URxvt.font: 		    xft:Roboto Mono:pixelsize=12:style=medium
URxvt.letterSpace:          -1
URxvt.lineSpace:	    2
URxvt.cursorUnderline:	    true

URxvt.scrollBar:	    off
URxvt.scrollStyle:	    plain
URxvt.depth:		    32
URxvt.geometry: 	    180x100
URxvt.fading:		    40

URxvt.borderColor: 	    rgba:0000/0000/0000/e800
URxvt.internalBorder:       10

! Tabs
! How to customize tabs: http://forums.debian.net/viewtopic.php?f=16&t=53495
! Perl extension: http://jbl.web.cern.ch/jbl/doc/urxvt/
URxvt.perl-ext-common:      default,tabbed
URxvt.tabbed.tabbar-fg:     12
URxvt.tabbed.tabbar-bg:     -1
URxvt.tabbed.tab-fg: 	    10
URxvt.tabbed.tab-bg: 	    -1
URxvt.tabbed.autohide:      true
URxvt.tabbed.new-button:    false
URxvt.tabbed.title: 	    true

! Colors
URxvt.background: 	    rgba:0000/0000/0000/e800
URxvt.foreground: 	    #cccccc
URxvt.cursorColor: 	    #00ACC1

! grey 900, 600
URxvt.color0: #212121
URxvt.color8: #757575

! red 600, 400
URxvt.color1: #E53935
URxvt.color9: #EF5350

! green 600, 400
URxvt.color2: #7CB342
URxvt.color10: #9CCC65

! amber 600, 400
URxvt.color3: #FFB300
URxvt.color11: #FFCA28

! blue 600, 400
URxvt.color4: #039BE5
URxvt.color12: #29B6F6

! teal 600, 400
URxvt.color5: #00897B
URxvt.color13: #26A69A

! cyan 600, 400
URxvt.color6: #00ACC1
URxvt.color14: #26C6DA

! grey 400, white
URxvt.color7: #BDBDBD
URxvt.color15: #FFFFFF